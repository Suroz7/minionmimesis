{
  "name": "mimesis-minion",
  "displayName": "Minionmimesis",
  "description": "A VS Code extension to chat with LLaMA 3.2",
  "version": "0.0.6",
  "publisher": "Mimesis",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:minion.MinionsAssemble"
  ],
  "main": "./out/extension.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Suroz7/minionmimesis"
  },
  "contributes": {
    "commands": [
      {
        "command": "minion.MinionsAssemble",
        "title": "Minions Assemble!"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "package": "vsce package"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/mocha": "^10.0.10",
    "jest": "^29.7.0",
    "mocha": "^11.1.0",
    "ts-jest": "^29.2.5",
    "typescript": "^4.4.3",
    "vsce": "^2.3.3",
    "vscode": "^1.1.37"
  },
  "dependencies": {
    "axios": "^0.21.1"
  }
}
